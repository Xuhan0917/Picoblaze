{
    "project":"Basic Picoblaze Project",
    "flow_steps": {
	"1" : "clean",
	"2" : "xvlog",
    "3" : "xelab",
    "4" : "xsim"
    },
    "flow" : {
	"clean" :{
	    "executable" : "rm",
	    "arguments" : "-rf *.log *.pb *~ webtalk* xsim* "
	},
	"xvlog" :{
	    "executable" : "xvlog",
        "arguments" : " -prj sim.f"
	},
    "xelab" :{
        "executable":"xelab",
        "arguments" : "work.glbl work.testbench -L unisims_ver -L unimacro_ver -L xilinxcorelib_ver -prj sim.f -log elaborate.log -debug all"
    },
	"xsim" : {
	    "executable": "xsim",
	    "arguments" : " -tclbatch testbench.tcl work.glbl#work.testbench -log simulate.log"
	},
	"run" : {
	    "executable" : "./a.out",
	    "arguments"  : " "
	}
    }
}
